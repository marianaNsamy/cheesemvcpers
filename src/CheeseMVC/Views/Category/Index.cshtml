@model List<CheeseMVC.Models.CheeseCategory>
<h1>@ViewBag.Title</h1>
<ul>
    @foreach (var category in Model)
    {
            <li>ID: @category.ID</li>
            <li>Name: @category.Name</li>
    }
</ul>
@if (Model.Count == 0)
{
    <p>No category yet!</p>
}
<p><a asp-controller="Category" asp-action="Add">Add Category</a></p>
